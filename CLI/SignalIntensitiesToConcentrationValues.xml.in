<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Converters</category>
  <title>Signal Intensities To Concentration Values</title>
  <index>2</index>
  <description><![CDATA[Convert signal intensities to concentration values.\n\n]]></description>
  <version>0.1.0.$Revision: 19276 $(alpha)</version>
  <documentation-url>http://wiki.slicer.org/slicerWiki/index.php/Documentation/4.0/Modules/ConvertSignalIntensityToConcentration</documentation-url>
  <license/>
  <contributor>Yingxuan Zhu, James V. Miller</contributor>
  <acknowledgements><![CDATA[This module was developed by Yingxuan Zhu at GE Research with contributions from James V. Miller.\n\n.]]></acknowledgements>
  <parameters>
    <label>PkModeling Parameters</label>
    <description><![CDATA[Parameters used for registration]]></description>
    <boolean hidden="true">
      <name>TestingMode</name>
      <longflag>testingmode</longflag>
      <description><![CDATA[Enable testing mode.]]></description>
      <label>Testing mode</label>
      <default>false</default>
    </boolean>      
    <float>
      <name>T1PreValue</name>   
      <description><![CDATA[T1 value.]]></description>
      <longflag>T1</longflag>      
      <label>T1 Value</label>
      <channel>input</channel>
      <default>1600</default>      
    </float>    
    <float>
      <name>TRValue</name>   
      <description><![CDATA[TR value.]]></description>
      <longflag>TR</longflag>      
      <label>TR Value</label>
      <channel>input</channel>
      <default>3.984</default>      
    </float>
	    <float>
      <name>FAValue</name>   
      <description><![CDATA[FA value.]]></description>
      <longflag>FA</longflag>      
      <label>FA Value</label>
      <channel>input</channel>
      <default>15</default>      
    </float>
    <float>
      <name>RelaxivityValue</name>   
      <description><![CDATA[Relaxivity value.]]></description>
      <longflag>relaxivity</longflag>      
      <label>Relaxivity Value</label>
      <channel>input</channel>
      <default>0.0049</default>      
    </float>
	<float>
      <name>S0GradValue</name>   
      <description><![CDATA[Signal Gradient Threshold value.]]></description>
      <longflag>S0grad</longflag>      
      <label>Signal Gradient Threshold Value</label>
      <channel>input</channel>
      <default>15.0</default>      
    </float>	
  </parameters>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>    
    <image>
      <name>InputFourDNrrdFile</name>
      <longflag>inputFourDNrrd</longflag>
      <label>Input Volume</label>
      <channel>input</channel>      
      <description><![CDATA[Input 4D volume in nrrd, the size is x*y*z*t.]]></description>
    </image>
	<image>
      <name>OutputFourDNrrdFile</name>
      <longflag>outputFourDNrrd</longflag>
      <label>Output Volume</label>
      <channel>output</channel>      
      <description><![CDATA[Output 4D volume in nrrd, the size is x*y*z*t.]]></description>
    </image>		
  </parameters>
</executable>
